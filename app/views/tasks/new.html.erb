<h1>予定作成ページ</h1>

<div class="row">
  <div class="col-xs-12, col-sm-offset-2 col-sm-8, col-lg-offset-3 col-lg-6">
    
<%= form_for(@task) do |t| %>

<div class="form-group">
<%= t.label :status, '状態' %>

<%= t.text_field :status, class: 'form-control'%>
</div>


<div class="form-group">
<%= t.label :content, '予定' %>

<%= t.text_field :content, class:'form-control' %>
</div>

<%= t.submit '書き込む', class: 'btn btn-primary'%>

<% end %>

  </div>
</div>